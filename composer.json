{
  "name": "veem/sdk",
  "description": "Veem PHP SDK",
  "keywords": [
    "veem",
    "api",
    "sdk",
    "global payment processing"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Veem\\" : "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Veem\\": "lib/"
    }
  },
  "require-dev": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "phpunit/phpunit": "^8.3.5"
  }
}
